{{outlet}}

<h2> New Calendar </h2>

<div class="form">
  <form>
    <p> <label for="name">Name: </label> </p>
    <p> {{input value=name type='text' id='name'}} </p>

    <p> <label for="notes">Notes (optional): </label> </p>
    <p> {{textarea value=notes id='notes'}} </p>

    <p>
      <button type="button" class="btn" {{action 'save'}}>Save</button>
      <button type="button" class="btn" {{action 'cancel'}}>Cancel</button>
    </p>
  </form>
</div>
